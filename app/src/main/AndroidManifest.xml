<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.idbs.football">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Football">
        <activity
            android:name=".ListComposeActivity"
            android:exported="false"
            android:label="@string/title_activity_list_compose"
            android:theme="@style/Theme.Football.NoActionBar" />
        <activity
            android:name=".ComposeActivity1"
            android:exported="true"
            android:label="@string/title_activity_compose1"
            android:theme="@style/Theme.Football.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"></activity>
    </application>

</manifest>